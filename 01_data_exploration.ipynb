yaml
# Project Configuration

# Data paths
data:
  raw: "data/raw/"
  processed: "data/processed/"
  train_file: "train.csv"
  test_file: "test.csv"
  store_file: "store.csv"

# Model parameters
model:
  random_state: 42
  test_size: 0.2
  cv_folds: 5
  
  xgboost:
    n_estimators: 1000
    learning_rate: 0.05
    max_depth: 7
    min_child_weight: 1
    subsample: 0.8
    colsample_bytree: 0.8
    
  random_forest:
    n_estimators: 500
    max_depth: 20
    min_samples_split: 10
    min_samples_leaf: 4

# Feature engineering
features:
  lag_features: [1, 7, 14, 30]
  rolling_windows: [7, 14, 30]
  date_features:
    - year
    - month
    - day
    - dayofweek
    - weekofyear
    - quarter

# Evaluation metrics
metrics:
  - rmspe
  - mae
  - rmse
  - r2_score

# Visualization
plotting:
  style: "seaborn"
  figure_size: [12, 6]
  save_format: "png"
  dpi: 300
